{
  "name": "esptk",
  "version": "2.0.4",
  "description": "read information from gamebryo esp/esm/esl files (very basic)",
  "repository": {
    "type": "git",
    "url": "https://github.com/Nexus-Mods/node-esptk.git"
  },
  "scripts": {
    "upload": "prebuild.cmd -r napi -t 4 -a x64 --prepack codesign",
    "rebuild": "autogypi && node-gyp configure build",
    "install": "prebuild-install -r napi -t 4 -a x64 || npm run rebuild"
  },
  "binary": {
    "napi_versions": [
      4
    ]
  },
  "author": "Black Tree Gaming Ltd.",
  "license": "GPL-3.0",
  "devDependencies": {
    "prebuild": "^11.0.3"
  },
  "dependencies": {
    "autogypi": "^0.2.2",
    "esptk": "TanninOne/modorganizer-esptk#noboost",
    "node-addon-api": "^5.0.0",
    "node-gyp": "^9.0.0",
    "prebuild-install": "^7.1.0"
  }
}
